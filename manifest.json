
{
  "manifest_version": 2,
  "name": "Keibo",
  "version": "1.0",
  "description": "Your extension description",
  
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["newcontent.js"]
    }
  ],
  
  "web_accessible_resources": [
    "toolbar.html"
  ]
}